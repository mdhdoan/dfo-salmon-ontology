@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ro:      <http://purl.obolibrary.org/obo/RO> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix geo:     <http://www.w3.org/2003/01/geo/wgs84pos#> .

### Root classes
:Environmental a rdfs:Class ;
    rdfs:label "Environmental Factors" ;
    :description "Environmental features relevant to salmon stream inspections." .

### Stream Condition
:StreamCondition a rdfs:Class ;
    rdfs:subClassOf :Environmental ;
    rdfs:label "Stream Condition" ;
    skos:definition "Dynamic states or disturbances affecting the stream or river at the time of inspection." ;
    dcterms:source <https://en.wikipedia.org/wiki/Stream_hydrology> ;
    :description "Covers hydrological and geomorphic conditions observed during inspection." .

### Stream Condition
:AtmosphericCondition a rdfs:Class ;
    rdfs:subClassOf :Environmental ;
    rdfs:label "Atmospheric Condition" ;
    skos:definition "Dynamic Atmospheric states or disturbances affecting the stream or river at the time of inspection." ;
    :description "Covers cloudiness and brightness and weather observed during inspection." .

### ——— Inspection branch ———

:Inspection a rdfs:Class ;
  rdfs:label "Inspection" ;
  :description "A single field observation event capturing methods, counts, time, and observations." .

:InspectionMethod a rdfs:Class ;
  rdfs:label "Inspection Method" ;
  :description "Controlled list of methods (e.g., visual survey, snorkel, drone, eDNA)." .

:hasMethod a rdfs:Property ;
  rdfs:label "has method" ;
  rdfs:domain :Inspection ;
  rdfs:range  :InspectionMethod ;
  :description "Links an inspection event to the method used." .

:observedAtWaterBody a rdfs:Property ;
  rdfs:label "observed at water body" ;
  rdfs:domain :Inspection ;
  rdfs:range  :Environmental ;
  :description "Water body type/location associated with the inspection." .

:observedSalmonAspect a rdfs:Property ;
  rdfs:label "observed salmon aspect" ;
  rdfs:domain :Inspection ;
  rdfs:range  :Salmon ;
  :description "Which salmon-related concept was observed (e.g., Redd, Jack, PredatorObservation)." .

:observedCondition a rdfs:Property ;
  rdfs:label "observed condition" ;
  rdfs:domain :Inspection ;
  rdfs:range  :StreamCondition ;
  :description "Environmental condition(s) observed during the inspection (e.g., LowFlow, StreamObstruction)." .

:fishCount a rdfs:DatatypeProperty ;
  rdfs:label "fish count" ;
  rdfs:domain :Inspection ;
  rdfs:range  xsd:integer ;
  :description "Total number of fish observed (or method-specific count)." .

:inspectionTime a rdfs:DatatypeProperty ;
  rdfs:label "inspection time" ;
  rdfs:domain :Inspection ;
  rdfs:range  xsd:dateTime ;
  :description "Timestamp for the observation." .

:notes a rdfs:DatatypeProperty ;
  rdfs:label "notes" ;
  rdfs:domain :Inspection ;
  rdfs:range  xsd:string ;
  :description "Free-text notes captured during the inspection." .

### ——— Inspection Methods: Ground vs Aerial ———

:GroundMethod a rdfs:Class ;
  rdfs:subClassOf :InspectionMethod ;
  rdfs:label "Ground-based Method" ;
  :description "Methods conducted from the ground or water surface at site level." .

:AerialMethod a rdfs:Class ;
  rdfs:subClassOf :InspectionMethod ;
  rdfs:label "Aerial Method" ;
  :description "Methods conducted from aircraft for wide-area or rapid assessment." .

### Ground methods
:SCBankwalk a rdfs:Class ;
  rdfs:subClassOf :GroundMethod ;
  rdfs:label "South Coast - Bankwalk" ;
  :description "South Coast - Observers walk stream banks recording salmon, redds, and conditions." .

:SCStreamWalk a rdfs:Class ;
  rdfs:subClassOf :GroundMethod ;
  rdfs:label "South Coast - Stream Walk" ;
  :description "South Coast - Wading or along-channel walking to observe fish and habitat." .

:SCSnorkelSurvey a rdfs:Class ;
  rdfs:subClassOf :GroundMethod ;
  rdfs:label "South Coast - Snorkel" ;
  :description "South Coast - In-water snorkeling to enumerate/identify fish and assess habitat." .

:SCBoatSurvey a rdfs:Class ;
  rdfs:subClassOf :GroundMethod ;
  rdfs:label "South Coast - Boat Survey" ;
  :description "South Coast - Boat-based visual survey of reaches unsuitable for walking/snorkel." .

:SCDeadPitch a rdfs:Class ;
  rdfs:subClassOf :GroundMethod ;
  rdfs:label "South Coast - Dead Pitch" ;
  :description "South Coast - Carcass (dead fish) tally used for escapement or mortality assessment." .

:FIACOHOStreamWalk a rdfs:Class ;
  rdfs:subClassOf :GroundMethod ;
  rdfs:label "FIACOHO - Stream Walk" ;
  :description """FIACOHO - Wading or along-channel walking to observe fish and habitat.
* Check segment break fieldcard to ensure you are surveying the correct area (some additional areas may be required near peak on noted systems)
* Conduct surveys in a downstream direction.  This is not the preferred method when conducting visual surveys; however, it is necessary to due to time constraints and logistics.
* Avoid walking on redds and near fish on redds.
* Investigate all hiding areas within a Segment carefully and thoroughly (e.g. logjams, cutbanks, over-hanging shrubs/grass, pools, etc.) to ensure the detection of Coho.
  - Coho are notorious for their ability to hide – if you think there is no way a Coho will be there CHECK IT ANYWAY!!
  - Check upstream of all beaver dams to confirm Coho presence/absence – do not assume a beaver dam is impassable.
  - Take your time when counting – do several counts of pools before recording the number of fish observed on your tally counter(s).  Move to the best viewing spot (lighting, height, etc.) 
    when counting not just what is convenient and/or easiest for you.  Gently probe hiding areas with your peugh/wading staff or “rock” areas you cannot reach.
* Conduct a minimum of 7 surveys per system (at a minimum, 5 high reliability counts are required for the AUC calcuation).
* Survey systems on a 7-day rotation (survey interval).
* Record the number of holders, spawners and carcasses observed on a Stream Inspection Log (SIL) field-card.
* Record all unusual fish behaviour and environmental conditions observed on the SIL.
* Surveyor efforts need to be consistent from one survey to the next and from one system to another. 
""" .

:FIACOHOBoatSurvey a rdfs:Class ;
  rdfs:subClassOf :GroundMethod ;
  rdfs:label "FIACOHO - Boat Survey" ;
  :description "FIACOHO - Boat-based visual survey of reaches unsuitable for walking/snorkel." .

:FIACOHOBankwalk a rdfs:Class ;
  rdfs:subClassOf :GroundMethod ;
  rdfs:label "FIACOHO - Bankwalk" ;
  :description "FIACOHO - Observers walk stream banks recording salmon, redds, and conditions." .

:WCVIBankwalk a rdfs:Class ;
  rdfs:subClassOf :GroundMethod ;
  rdfs:label "WCVI - Bankwalk" ;
  :description "WCVI - Observers walk stream banks recording salmon, redds, and conditions." .

:WCVIStreamWalk a rdfs:Class ;
  rdfs:subClassOf :GroundMethod ;
  rdfs:label "WCVI - Stream Walk" ;
  :description "WCVI - Wading or along-channel walking to observe fish and habitat." .

:WCVISnorkelSurvey a rdfs:Class ;
  rdfs:subClassOf :GroundMethod ;
  rdfs:label "WCVI - Snorkel" ;
  :description "WCVI - In-water snorkeling to enumerate/identify fish and assess habitat." .

:WCVIDeadPitch a rdfs:Class ;
  rdfs:subClassOf :GroundMethod ;
  rdfs:label "WCVI - Dead Pitch" ;
  :description "WCVI - Carcass (dead fish) tally used for escapement or mortality assessment." .

:WCVIBoatSurvey a rdfs:Class ;
  rdfs:subClassOf :GroundMethod ;
  rdfs:label "WCVI - Boat Survey" ;
  :description "WCVI - Boat-based visual survey of reaches unsuitable for walking/snorkel." .

:BC16Bankwalk a rdfs:Class ;
  rdfs:subClassOf :GroundMethod ;
  rdfs:label "BC16 - Bankwalk" ;
  :description "BC16 - Observers walk stream banks recording salmon, redds, and conditions." .

:BC16StreamWalk a rdfs:Class ;
  rdfs:subClassOf :GroundMethod ;
  rdfs:label "BC16 - Stream Walk" ;
  :description "BC16 - Wading or along-channel walking to observe fish and habitat." .

:BC16SnorkelSurvey a rdfs:Class ;
  rdfs:subClassOf :GroundMethod ;
  rdfs:label "BC16 - Snorkel" ;
  :description "BC16 - In-water snorkeling to enumerate/identify fish and assess habitat." .

:BC16BoatSurvey a rdfs:Class ;
  rdfs:subClassOf :GroundMethod ;
  rdfs:label "BC16 - Boat Survey" ;
  :description "BC16 - Boat-based visual survey of reaches unsuitable for walking/snorkel." .

:BC16ReddCount a rdfs:Class ;
  rdfs:subClassOf :GroundMethod ;
  rdfs:label "BC16 - Redd Count" ;
  :description "BC16 - Standardized enumeration of redds within defined reaches." .

:BC16DeadPitch a rdfs:Class ;
  rdfs:subClassOf :GroundMethod ;
