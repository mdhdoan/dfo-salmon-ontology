@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Salmon a rdfs:Class ;
    rdfs:label "Salmon Characteristics" ;
    :description "Biological states, observations, and pressures relevant to salmon during inspections." .

:SalmonSpecies a rdfs:Class ;
    rdfs:label "Salmon Species" ;
    :description "Covers species-specific observations, linked to habitat and inspection records." ;
    dcterms:source <https://en.wikipedia.org/wiki/Salmon> ;
    rdfs:subClassOf :Salmon ;
    skos:definition "Taxonomic species of salmon observed during inspections." .

:AtlanticSalmon a rdfs:Class ;
    rdfs:subClassOf :SalmonSpecies ;
    rdfs:label "Atlantic Salmon" ;
    :description "Only salmon native to the Atlantic; widely farmed." ;
    :foundInRegion :Europe,
        :NorthAmerica,
        :NorthAtlantic ;
    :scientificName "Salmo salar" ;
    dcterms:source <https://en.wikipedia.org/wiki/Atlantic_salmon> ;
    skos:altLabel "Salmo salar" .

:ChinookSalmon a rdfs:Class ;
    rdfs:subClassOf :SalmonSpecies ;
    rdfs:label "Chinook Salmon" ;
    :description "Largest Pacific salmon species; highly valued for size and taste." ;
    :foundInRegion :Alaska,
        :BritishColumbia,
        :California,
        :NorthAmerica,
        :NorthPacific,
        :NortheastAsia,
        :Russia,
        :WashingtonState ;
    :scientificName "Oncorhynchus tshawytscha" ;
    dcterms:source <https://en.wikipedia.org/wiki/Chinook_salmon> ;
    skos:altLabel "Blackmouth", "King Salmon", "Quinnat Salmon", "Spring Salmon", "Tyee Salmon" .

:ChumSalmon a rdfs:Class ;
    rdfs:subClassOf :SalmonSpecies ;
    rdfs:label "Chum Salmon" ;
    :description "Noted for large canine-like teeth during spawning; important subsistence fish." ;
    :foundInRegion :Alaska,
        :BritishColumbia,
        :NorthPacific,
        :NortheastAsia,
        :Russia ;
    :scientificName "Oncorhynchus keta" ;
    dcterms:source <https://en.wikipedia.org/wiki/Chum_salmon> ;
    skos:altLabel "Dog Salmon",
        "Keta Salmon" .

:CohoSalmon a rdfs:Class ;
    rdfs:subClassOf :SalmonSpecies ;
    rdfs:label "Coho Salmon" ;
    :description "Known for acrobatic behavior and silver body; popular sport fish." ;
    :foundInRegion :Alaska,
        :BritishColumbia,
        :California,
        :NorthPacific,
        :NortheastAsia,
        :WashingtonState ;
    :scientificName "Oncorhynchus kisutch" ;
    dcterms:source <https://en.wikipedia.org/wiki/Coho_salmon> ;
    skos:altLabel "Silver Salmon" .

:PinkSalmon a rdfs:Class ;
    rdfs:subClassOf :SalmonSpecies ;
    rdfs:label "Pink Salmon" ;
    :description "Smallest and most abundant Pacific salmon; two-year life cycle." ;
    :foundInRegion :Alaska,
        :BritishColumbia,
        :NorthPacific,
        :Russia,
        :Siberia,
        :WashingtonState ;
    :scientificName "Oncorhynchus gorbuscha" ;
    dcterms:source <https://en.wikipedia.org/wiki/Pink_salmon> ;
    skos:altLabel "Humpback Salmon",
        "Humpies" .

:SockeyeSalmon a rdfs:Class ;
    rdfs:subClassOf :SalmonSpecies ;
    rdfs:label "Sockeye Salmon" ;
    :description "Renowned for bright red spawning color and critical role in ecosystems." ;
    :foundInRegion :Alaska,
        :BritishColumbia,
        :NorthPacific,
        :Russia ;
    :scientificName "Oncorhynchus nerka" ;
    dcterms:source <https://en.wikipedia.org/wiki/Sockeye_salmon> ;
    skos:altLabel "Blueback Salmon",
        "Red Salmon" .