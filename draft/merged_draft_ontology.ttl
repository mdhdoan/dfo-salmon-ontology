### Begin: draft_salmon_information.ttl
### Root classes
:Environmental a rdfs:Class ;
    rdfs:label "Environmental Factors" ;
    :description "Environmental features relevant to salmon stream inspections." .

:InspectionDetails a rdfs:Class ;
    rdfs:label "Inspection Details" .
### ——— Salmon branch ———

:Salmon a rdfs:Class ;
  rdfs:label "Salmon Characteristics" ;
  :description "Biological states, observations, and pressures relevant to salmon during inspections." .

### Relation for salmon observations → impacts
:indicativeOfImpact a rdfs:Property ;
  rdfs:label "indicative of impact" ;
  rdfs:domain :Salmon ;
  rdfs:range  :Impact ;
  :description "Links a salmon observation to an impact it suggests is present (e.g., predation pressure, sedimentation)." .

### Additional impact for predators
:PredationPressure a rdfs:Class ;
  rdfs:subClassOf :Impact ;
  rdfs:label "Predation Pressure" ;
  skos:definition "Elevated risk of mortality or injury to salmon due to predator presence and activity." ;
  dcterms:source <https://en.wikipedia.org/wiki/Predation> ;
  :description "Use when predators are actively foraging or concentrated near salmon." .

### Life stages & reproduction
:Juvenile a rdfs:Class ;
  rdfs:subClassOf :Salmon ;
  rdfs:label "Juvenile" ;
  skos:definition "A young salmon prior to reaching sexual maturity." ;
  dcterms:source <https://en.wikipedia.org/wiki/Juvenile_(organism)> ;
  :description "Covers fry, parr, and smolt observations if not otherwise specified." .

:Jack a rdfs:Class ;
  rdfs:subClassOf :Salmon ;
  rdfs:label "Jack" ;
  skos:definition "A precociously maturing male salmon that returns to spawn at a younger age and smaller size than typical adults." ;
  dcterms:source <https://en.wikipedia.org/wiki/Chinook_salmon#Life_cycle> ;
  :description "Often observed among returning males; size-based field notes can identify jacks." .

:Redd a rdfs:Class ;
  rdfs:subClassOf :Salmon ;
  rdfs:label "Redd" ;
  skos:definition "A nest excavated by a female salmon in gravel substrate for depositing eggs." ;
  dcterms:source <https://en.wikipedia.org/wiki/Redd> ;
  :description "Record presence/condition of redds in spawning reaches." .

### Demography & condition
:VariationInSexRatio a rdfs:Class ;
  rdfs:subClassOf :Salmon ;
  rdfs:label "Variation in Sex Ratio" ;
  skos:definition "An observed deviation from the expected proportion of males to females in a salmon group." ;
  dcterms:source <https://en.wikipedia.org/wiki/Sex_ratio> ;
  :description "Note skew (e.g., male-biased, female-biased) and context (run timing, location)." ;
  :indicativeOfImpact :ReducedConnectivity .

:UnusualMortality a rdfs:Class ;
  rdfs:subClassOf :Salmon ;
  rdfs:label "Unusual Mortalities" ;
  skos:definition "Elevated or atypical mortality of salmon beyond expected baseline for the season or site." ;
  dcterms:source <https://en.wikipedia.org/wiki/Fish_kill> ;
  :description "Use when mortality appears clustered, sudden, or cause-specific (e.g., thermal, disease)." ;
  :indicativeOfImpact :ReducedConnectivity .

### Habitat pressure
:SpawningHabitatDegradation a rdfs:Class ;
  rdfs:subClassOf :Salmon ;
  rdfs:label "Spawning Habitat Degradation" ;
  skos:definition "A decline in the quality of gravel spawning habitat due to factors such as fine sediment, scouring, or pollution." ;
  dcterms:source <https://en.wikipedia.org/wiki/Habitat_destruction> ;
  :description "Link to observed environmental drivers (e.g., bank erosion, flood) if known." ;
  :indicativeOfImpact :IncreasedSedimentation , :SpawningHabitatScour .

### Biological activity level
:BiologicalActivity a rdfs:Class ;
  rdfs:subClassOf :Salmon ;
  rdfs:label "Biological Activities" ;
  skos:definition "Observed level of salmon biological activity (e.g., feeding, holding, spawning) during inspection." ;
  dcterms:source <https://en.wikipedia.org/wiki/Salmon_run> ;
  :description "Use subclasses to categorize intensity." .

:EnhancedActivity a rdfs:Class ;
  rdfs:subClassOf :BiologicalActivity ;
  rdfs:label "Enhanced Activity" ;
  skos:definition "Activity levels above typical expectations for the site and season." ;
  dcterms:source <https://en.wikipedia.org/wiki/Animal_behavior> ;
  :description "Examples: unusually dense holding/spawning, strong aggregation." .

:IntenseActivity a rdfs:Class ;
  rdfs:subClassOf :BiologicalActivity ;
  rdfs:label "Intense Activity" ;
  skos:definition "Markedly elevated activity characterized by conspicuous behaviors (e.g., active spawning, aggressive interactions)." ;
  dcterms:source <https://en.wikipedia.org/wiki/Animal_behavior> ;
  :description "Consider noting trigger (flow spike, temperature change)." .

### Predators
:PredatorObservation a rdfs:Class ;
  rdfs:subClassOf :Salmon ;
  rdfs:label "Predators" ;
  skos:definition "Observation of predators interacting with or near salmon." ;
  dcterms:source <https://en.wikipedia.org/wiki/Predation> ;
  :description "Document predator type and activity (foraging, presence, scavenging)." ;
  :indicativeOfImpact :PredationPressure .

:BearPredator a rdfs:Class ;
  rdfs:subClassOf :PredatorObservation ;
  rdfs:label "Bear" ;
  skos:definition "Observation of bears preying upon or scavenging salmon." ;
  dcterms:source <https://en.wikipedia.org/wiki/Brown_bear#Diet> ;
  :description "Common along spawning streams; note intensity and location." ;
  :indicativeOfImpact :PredationPressure .

:SealPredator a rdfs:Class ;
  rdfs:subClassOf :PredatorObservation ;
  rdfs:label "Seal" ;
  skos:definition "Observation of seals preying upon salmon, typically in estuarine or nearshore reaches." ;
  dcterms:source <https://en.wikipedia.org/wiki/Harbor_seal#Diet> ;
  :description "Record species if known (e.g., harbor seal) and behavior." ;
  :indicativeOfImpact :PredationPressure .

### Species (with region model)

:SalmonSpecies a rdfs:Class ;
  rdfs:subClassOf :Salmon ;
  rdfs:label "Salmon Species" ;
  skos:definition "Taxonomic species of salmon observed during inspections." ;
  dcterms:source <https://en.wikipedia.org/wiki/Salmon> ;
  :description "Covers species-specific observations, linked to habitat and inspection records." .

:scientificName a rdfs:DatatypeProperty ;
  rdfs:label "scientific name" ;
  rdfs:domain :SalmonSpecies ;
  rdfs:range  xsd:string ;
  :description "The Latin binomial name of the salmon species." .

:foundInRegion a rdfs:Property ;
  rdfs:label "found in region" ;
  rdfs:domain :SalmonSpecies ;
  rdfs:range  :Region ;
  :description "Links a salmon species to regions where it is typically found." .

