@prefix :        <http://example.org/salmon-inspections#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ro:      <http://purl.obolibrary.org/obo/RO> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix geo:     <http://www.w3.org/2003/01/geo/wgs84pos#> .

### ——— Inspection branch ———

:Inspection a rdfs:Class ;
  rdfs:label "Inspection" ;
  :description "A single field observation event capturing methods, counts, time, and observations." .

:InspectionMethod a rdfs:Class ;
  rdfs:label "Inspection Method" ;
  :description "Controlled list of methods (e.g., visual survey, snorkel, drone, eDNA)." .

:hasMethod a rdfs:Property ;
  rdfs:label "has method" ;
  rdfs:domain :Inspection ;
  rdfs:range  :InspectionMethod ;
  :description "Links an inspection event to the method used." .

:observedAtWaterBody a rdfs:Property ;
  rdfs:label "observed at water body" ;
  rdfs:domain :Inspection ;
  rdfs:range  :Environmental ;
  :description "Water body type/location associated with the inspection." .

:observedSalmonAspect a rdfs:Property ;
  rdfs:label "observed salmon aspect" ;
  rdfs:domain :Inspection ;
  rdfs:range  :Salmon ;
  :description "Which salmon-related concept was observed (e.g., Redd, Jack, PredatorObservation)." .

:observedCondition a rdfs:Property ;
  rdfs:label "observed condition" ;
  rdfs:domain :Inspection ;
  rdfs:range  :StreamCondition ;
  :description "Environmental condition(s) observed during the inspection (e.g., LowFlow, StreamObstruction)." .

:fishCount a rdfs:DatatypeProperty ;
  rdfs:label "fish count" ;
  rdfs:domain :Inspection ;
  rdfs:range  xsd:integer ;
  :description "Total number of fish observed (or method-specific count)." .

:inspectionTime a rdfs:DatatypeProperty ;
  rdfs:label "inspection time" ;
  rdfs:domain :Inspection ;
  rdfs:range  xsd:dateTime ;
  :description "Timestamp for the observation." .

:notes a rdfs:DatatypeProperty ;
  rdfs:label "notes" ;
  rdfs:domain :Inspection ;
  rdfs:range  xsd:string ;
  :description "Free-text notes captured during the inspection." .

### ——— Inspection Methods: Ground vs Aerial ———

:GroundMethod a rdfs:Class ;
  rdfs:subClassOf :InspectionMethod ;
  rdfs:label "Ground-based Method" ;
  :description "Methods conducted from the ground or water surface at site level." .

:AerialMethod a rdfs:Class ;
  rdfs:subClassOf :InspectionMethod ;
  rdfs:label "Aerial Method" ;
  :description "Methods conducted from aircraft for wide-area or rapid assessment." .

