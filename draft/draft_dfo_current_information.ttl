### ——— Inspection Details branch ———

:InspectionDetails a rdfs:Class ;
  rdfs:label "Inspection Details" ;
  :description "Controlled vocabularies, roles, and metadata used to describe inspection events." .

### Core vocab buckets
:InspectionMethod a rdfs:Class ;
  rdfs:subClassOf :InspectionDetails ;
  rdfs:label "Inspection Method" ;
  :description "Standardized methods (e.g., visual survey, snorkel, helicopter)." .

:DetectionMethod a rdfs:Class ;
  rdfs:subClassOf :InspectionDetails ;
  rdfs:label "Detection Method" ;
  :description "How organisms/evidence were detected (visual, acoustic, imagery) to conduct inspections." .

:Gear a rdfs:Class ;
  rdfs:subClassOf :InspectionDetails ;
  rdfs:label "Gear" ;
  :description "Equipment used during inspection (e.g., camera, drone, GPS, waders)." .

:Protocol a rdfs:Class ;
  rdfs:subClassOf :InspectionDetails ;
  rdfs:label "Protocol" ;
  :description "Survey/recording standards or SOPs followed." .

:CountType a rdfs:Class ;
  rdfs:subClassOf :InspectionDetails ;
  rdfs:label "Count Type" ;
  :description "How counts were interpreted (absolute census vs. index/estimate)." .

:MediaObject a rdfs:Class ;
  rdfs:subClassOf :InspectionDetails ;
  rdfs:label "Media Object" ;
  :description "Photos, videos, audio, or documents linked to an inspection." .

:Person a rdfs:Class ;
  rdfs:subClassOf :InspectionDetails ;
  rdfs:label "Person" ;
  :description "Individual participating in the inspection (observer, reviewer, etc.)." .

:Organization a rdfs:Class ;
  rdfs:subClassOf :InspectionDetails ;
  rdfs:label "Organization" ;
  :description "Affiliation responsible for or participating in inspections." .

:Role a rdfs:Class ;
  rdfs:subClassOf :InspectionDetails ;
  rdfs:label "Role" ;
  :description "Functional role in the inspection workflow (Observer, Lead, QA Reviewer)." .

### Link properties from Inspection → these details
:hasMethod a rdfs:Property ;
  rdfs:label "has method" ;
  rdfs:domain :Inspection ;
  rdfs:range  :InspectionMethod ;
  :description "Links an inspection to a standardized method." .

:usedDetection a rdfs:Property ;
  rdfs:label "used detection" ;
  rdfs:domain :Inspection ;
  rdfs:range  :DetectionMethod ;
  :description "Detection approach used to observe salmon/evidence." .

:usedGear a rdfs:Property ;
  rdfs:label "used gear" ;
  rdfs:domain :Inspection ;
  rdfs:range  :Gear ;
  :description "Equipment used during the inspection." .

:followedProtocol a rdfs:Property ;
  rdfs:label "followed protocol" ;
  rdfs:domain :Inspection ;
  rdfs:range  :Protocol ;
  :description "Survey/SOP reference followed." .

:countType a rdfs:Property ;
  rdfs:label "count type" ;
  rdfs:domain :Inspection ;
  rdfs:range  :CountType ;
  :description "Indicates whether counts are absolute, indexed, estimated, etc." .

:effortValue a rdfs:DatatypeProperty ;
  rdfs:label "effort value" ;
  rdfs:domain :Inspection ;
  rdfs:range  xsd:decimal ;
  :description "Numeric effort magnitude (e.g., 2.5)." .

:observer a rdfs:Property ;
  rdfs:label "observer" ;
  rdfs:domain :Inspection ;
  rdfs:range  :Person ;
  :description "Primary person who made the observation(s)." .

:actedAs a rdfs:Property ;
  rdfs:label "acted as" ;
  rdfs:domain :Person ;
  rdfs:range  :Role ;
  :description "Role fulfilled by a specific person in the inspection." .

:affiliatedWith a rdfs:Property ;
  rdfs:label "affiliated with" ;
  rdfs:domain :Person ;
  rdfs:range  :Organization ;
  :description "Person’s organizational affiliation for the inspection." .

:mediaAttached a rdfs:Property ;
  rdfs:label "media attached" ;
  rdfs:domain :Inspection ;
  rdfs:range  :MediaObject ;
  :description "Media linked to the inspection (photos/videos/docs)." .

